{
	"name": "isaaclubow-theme",
	"type": "dark",
	"colors": {
		"editor.background": "#000000",
		"editor.foreground": "#dfdfdf",
		"activityBarBadge.background": "#b42c03",
		"sideBarTitle.foreground": "#bbbbbb"
	},
	"tokenColors": [
		{
			"name": "Bold Green",
			"scope": [
				"storage",
				"keyword",
				"support.function"
			],
			"settings": {
				"foreground": "#8fc266",
				"fontStyle": "bold"
			}
		},
		{
			"scope": [
				"constant.numeric"
			],
			"settings": {
				"foreground": "#fffb00"
			}
		},
		{
			"scope": [
				"keyword.other.unit.px.css",
				"punctuation",
				"meta.embedded.line.php punctuation",
            "meta.embedded.block.php punctuation",
            "entity.name.function"
			],
			"settings": {
				"foreground": "#d0d0d0",
				"fontStyle": "",
			}
		},
		{
			"scope": [
				"entity.name.tag.css",
				"string.quoted.double.html"
			],
			"settings": {
				"foreground": "#f0e2a2"
			}
		},
		{
			"name": "Comment Grey",
			"scope": [
				"comment",
				"comment punctuation",
				"comment support.class",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#666666"
			}
		},
		{
			"name": "Bold Grey",
			"scope": [
				"comment keyword"
			],
			"settings": {
				"foreground": "#888888",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Blue",
			"scope": [
				"variable",
				"variable punctuation",
				"punctuation.definition.variable.php"
			],
			"settings": {
				"foreground": "#81a8d4"
			}
		},
		{
			"name": "Bold Red",
			"scope": [
				"punctuation.section.embedded.begin.php",
				"punctuation.section.embedded.end.php"
			],
			"settings": {
				"foreground": "#ff0000",
				"fontStyle": "bold"
			}
		},
		{
			"scope": [
				"entity.other.attribute-name.class.css"
			],
			"settings": {
				"foreground": "#84ad62"
			}
		},
		{
			"name": "String Orange",
			"scope": [
				"string",
				"string punctuation",
				"entity.other.attribute-name.id.css",
				"punctuation.definition.string"
			],
			"settings": {
				"foreground": "#e6a631",
			}
		},
		{
			"scope": [
				"support.function.magic.php",
				"meta.object-literal.key",
				"support.type.property-name",
				"support.type.property-name punctuation",
				"variable.other.property",
				"entity.name.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"entity.other.attribute-name.html",
				"punctuation.separator.key-value.html",
				"punctuation.definition.string.begin.html",
				"punctuation.definition.string.end.html"
			],
			"settings": {
				"foreground": "#90b9bb",
				"fontStyle": ""
			}
		},
		{
			"name": "White Bold",
			"scope": [
				"keyword.operator"
			],
			"settings": {
				"foreground": "#fff",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Purple",
			"scope": [
				"entity.other.attribute-name.pseudo-class.css"
			],
			"settings": {
				"foreground": "#a478bd"
			}
		},
		{
			"name": "Bold Purple",
			"scope": [
				"storage.modifier",
				"keyword.control.at-rule.media.css"
			],
			"settings": {
				"foreground": "#a478bd",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Pale Red",
			"scope": [
				"entity.name.type.class.php",
				"support.class",
				"constant.other.php"
			],
			"settings": {
				"foreground": "#ff4040",
			}
		},
		{
			"name": "Pale Red Bold",
			"scope": [
				"constant.language"
			],
			"settings": {
				"foreground": "#ff4040",
				"fontStyle": "bold"
			}
		}
	]
}