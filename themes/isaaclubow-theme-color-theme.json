{
	"name": "isaaclubow-theme",
	"type": "dark",
	"colors": {
		"editor.background": "#070707",
		"editor.foreground": "#dfdfdf",
		"activityBarBadge.background": "#b42c03",
		"sideBarTitle.foreground": "#bbbbbb",
		"sideBar.background": "#0f0f0f",
		"sideBar.border": "#1a1a1a",
		"activityBar.background": "#0f0f0f",
		"statusBar.background": "#0f0f0f",
		"statusBar.border": "#1a1a1a",
		"activityBar.border": "#1a1a1a",
		"tab.inactiveBackground": "#171717",
		"tab.activeModifiedBorder": "#ff0000",
		"titleBar.activeBackground": "#0f0f0f",
		"titleBar.border": "#1a1a1a",
		"panel.background": "#0f0f0f",
		"panel.border": "#1a1a1a",
		"panelInput.border": "#1a1a1a",
	},
	"tokenColors": [
		{
			"name": "Bold Green",
			"scope": [
				"storage",
				"keyword",
				"support.function",
				"keyword.operator.type.php",
				"keyword.operator.new",
			],
			"settings": {
				"foreground": "#8fc266",
				"fontStyle": "bold",
			}
		},
		{
			"scope": [
            "constant.numeric",
            "keyword.other.important.css",
			],
			"settings": {
            "foreground": "#fffb00",
            "fontStyle": "",
			}
		},
		{
			"name": "Plain",
			"scope": [
				"constant.numeric.css",
				"keyword.other.unit",
				"keyword.operator.class",
				"support.function.url.css",
				"punctuation",
				"meta.embedded.line.php punctuation",
            "meta.embedded.block.php punctuation",
			],
			"settings": {
				"foreground": "#D0D0D0",
				"fontStyle": "",
			}
		},
		{
			"name": "Bold Grey",
			"scope": [
				"support.other.namespace punctuation.separator.inheritance",
			],
			"settings": {
				"foreground": "#aaaaaa",
				"fontStyle": "bold",
			}
		},
		{
			"scope": [
				"entity.name.tag.css",
				"string.quoted.double.html",
			],
			"settings": {
				"foreground": "#f0e2a2",
			}
		},
		{
			"name": "Comment Grey",
			"scope": [
				"comment",
				"comment punctuation",
				"comment support.class",
				"punctuation.definition.comment",
				"comment.block.documentation.phpdoc.php punctuation",
			],
			"settings": {
				"foreground": "#666666",
			}
		},
		{
			"name": "Bold Grey",
			"scope": [
				"comment keyword",
			],
			"settings": {
				"foreground": "#888888",
				"fontStyle": "bold",
			}
		},
		{
			"name": "Blue",
			"scope": [
				"variable",
				"variable punctuation",
				"punctuation.definition.variable.php",
				"string.quoted.double.php variable punctuation.definition.variable.php"
			],
			"settings": {
				"foreground": "#81a8d4",
			}
		},
		{
			"name": "Bold Red",
			"scope": [
				"punctuation.section.embedded.begin.php",
				"punctuation.section.embedded.end.php",
			],
			"settings": {
				"foreground": "#ff0000",
				"fontStyle": "bold",
			}
		},
		{
			"scope": [
				"entity.other.attribute-name.class.css",
			],
			"settings": {
				"foreground": "#84ad62",
			}
		},
		{
			"name": "String Orange",
			"scope": [
				"string",
				"string punctuation",
				"entity.other.attribute-name.id.css",
				"punctuation.definition.string",
			],
			"settings": {
				"foreground": "#e6a631",
			}
		},
		{
			"name": "Reddish Orange",
			"scope": [
				"support.other.namespace",
				"entity.name.type.namespace",
			],
			"settings": {
				"foreground": "#e7681f",
				"fontStyle": "bold",
			}
		},
		{
			"name": "Escape Orange Bold",
			"scope": [
				"constant.character.escape",
				"constant.character.entity",
				"punctuation.definition.entity.html",
				"string.quoted.double.php punctuation.definition.variable.php",
			],
			"settings": {
				"foreground": "#e46217",
				"fontStyle": "bold",
			}
		},
		{
			"name": "Pale Blue",
			"scope": [
				"support.function.magic.php",
				"meta.object-literal.key",
				"support.type.property-name",
				"support.type.property-name punctuation",
				"variable.other.property",
				"variable.other.object.property.js",
			],
			"settings": {
				"foreground": "#9acfd1",
				"fontStyle": "",
			}
		},
		{
			"name": "Paler Blue",
			"scope": [
				"entity.name.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"entity.other.attribute-name.html",
				"punctuation.separator.key-value.html",
				"punctuation.definition.string.begin.html",
				"punctuation.definition.string.end.html",
			],
			"settings": {
				"foreground": "#a8c4c5",
				"fontStyle": ""
			}
		},
		{
			"name": "White",
			"scope": [
				"entity.name.function",
			],
			"settings": {
				"foreground": "#fff",
				"fontStyle": ""
			}
		},
		{
			"name": "White Bold",
			"scope": [
				"keyword.operator",
			],
			"settings": {
				"foreground": "#fff",
				"fontStyle": "bold",
			}
		},
		{
			"name": "Purple",
			"scope": [
				"entity.other.attribute-name.pseudo-class.css",
				"entity.other.attribute-name.pseudo-element.css",
				"meta.tag.metadata.doctype entity.name.tag.html",
			],
			"settings": {
				"foreground": "#a478bd",
			}
		},
		{
			"name": "Bold Purple",
			"scope": [
				"storage.modifier",
				"keyword.control.at-rule.media.css",
			],
			"settings": {
				"foreground": "#a478bd",
				"fontStyle": "bold",
			}
		},
		{
			"name": "Pale Red",
			"scope": [
				"constant.other.class.php",
			],
			"settings": {
				"foreground": "#ff5252",
			}
		},
		{
			"name": "Pale Red Bold",
			"scope": [
				"entity.name.type.class.php",
				"entity.other.inherited-class.php",
				"support.class",
            "constant.language",
            "variable.other.global.php",
				"variable.other.global.php punctuation.definition.variable.php",
            "variable.other.global.safer.php",
				"variable.other.global.safer.php punctuation.definition.variable.php",
				"variable.language.this.php",
				"variable.language.this.php punctuation.definition.variable.php",
				"support.constant.ext.php",
				"support.constant.core.php",
				"support.constant.std.php",
				"support.constant",
				"constant.other.php",
			],
			"settings": {
				"foreground": "#ec4646",
				"fontStyle": "bold"
			}
		}
	]
}
