{
	"name": "isaaclubow-theme",
	"type": "dark",
	"colors": {
		"editor.background": "#000",
		"editor.foreground": "#dfdfdf",
		"activityBar.border": "#111",
		"activityBar.background": "#000",
		"activityBarBadge.background": "#b00",
		"sideBarTitle.foreground": "#ccc",
		"sideBarSectionHeader.background": "#000",
		"sideBar.background": "#000",
		"sideBar.border": "#111",
		"sideBar.foreground": "#ccc",
		"sideBarSectionHeader.border": "#111",
		"editorLineNumber.activeForeground": "#fff",
		"editorLineNumber.foreground": "#666",
		"statusBar.background": "#000",
		"statusBar.border": "#111",
		"tab.inactiveBackground": "#111",
		"tab.activeForeground": "#fff",
		"tab.inactiveForeground": "#777",
		"tab.border": "#111",
		"tab.activeBorderTop": "#333",
		"tab.activeModifiedBorder": "#b00",
		"titleBar.activeBackground": "#000",
		"titleBar.border": "#111",
		"panel.background": "#000",
		"panel.border": "#111",
		"panelInput.border": "#111",
		"panelSectionHeader.border": "#111",
		"panelTitle.inactiveForeground": "#999"
	},
	"tokenColors": [
		{
			"name": "Bold Green",
			"scope": [
				"storage",
				"keyword",
				"support.function",
				"support.class.builtin.js",
				"keyword.operator.type.php",
				"keyword.operator.new",
				"keyword.operator.expression.delete.js"
			],
			"settings": {
				"foreground": "#8fc266",
				"fontStyle": "bold"
			}
		},
		{
			"scope": [
				"constant.numeric",
				"keyword.other.important.css",
				"keyword.other.important.scss"
			],
			"settings": {
				"foreground": "#fffb00",
				"fontStyle": ""
			}
		},
		{
			"name": "Plain",
			"scope": [
				"keyword.other.unit",
				"keyword.operator.class",
				"support.function.url.css",
				"punctuation",
				"meta.embedded.line.php punctuation",
				"meta.embedded.block.php punctuation"
			],
			"settings": {
				"foreground": "#D0D0D0",
				"fontStyle": ""
			}
		},
		{
			"name": "Bold Grey",
			"scope": ["support.other.namespace punctuation.separator.inheritance"],
			"settings": {
				"foreground": "#aaa",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Tan",
			"scope": ["entity.name.tag.css", "string.quoted.double.html"],
			"settings": {
				"foreground": "#e2d7a8"
			}
		},
		{
			"name": "Comment Grey",
			"scope": [
				"comment",
				"comment punctuation",
				"comment support.class",
				"punctuation.definition.comment",
				"comment.block.documentation.phpdoc.php punctuation"
			],
			"settings": {
				"foreground": "#666"
			}
		},
		{
			"name": "Light Grey",
			"scope": ["variable.other.jsdoc"],
			"settings": {
				"foreground": "#999"
			}
		},
		{
			"name": "Bold Light Grey",
			"scope": [
				"comment keyword",
				"comment.block.documentation.phpdoc.php storage.modifier.php",
				"storage.type.class.jsdoc",
				"punctuation.definition.block.tag.jsdoc"
			],
			"settings": {
				"foreground": "#888",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Variable Blue",
			"scope": [
				"variable",
				"variable punctuation",
				"variable.js",
				"variable.other punctuation.definition.variable.php",
				"punctuation.definition.variable.php",
				"string.quoted.double.php variable punctuation.definition.variable.php",
				"string.quoted.double.sql.php variable punctuation.definition.variable.php"
			],
			"settings": {
				"foreground": "#81a8d4"
			}
		},
		{
			"name": "Bold Red",
			"scope": [
				"punctuation.section.embedded.begin.php",
				"punctuation.section.embedded.end.php"
			],
			"settings": {
				"foreground": "#ff0000",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Class Green",
			"scope": ["entity.other.attribute-name.class.css"],
			"settings": {
				"foreground": "#73af91"
			}
		},
		{
			"name": "String Orange",
			"scope": [
				"string",
				"string punctuation",
				"entity.other.attribute-name.id.css",
				"punctuation.definition.string",
				"string.regexp.single-quoted.php punctuation.definition.string",
				"string.regexp.double-quoted.php punctuation.definition.string"
			],
			"settings": {
				"foreground": "#d1a552"
			}
		},
		{
			"name": "Escape Orange Bold",
			"scope": [
				"support.other.namespace",
				"entity.name.type.namespace",
				"string.regexp.single-quoted.php",
				"string.regexp.double-quoted.php",
				"punctuation.definition.interpolation.begin.bracket.curly.scss",
				"punctuation.definition.interpolation.end.bracket.curly.scss",
				"constant.character.escape",
				"constant.character.entity",
				"support.constant.property-value.css",
				"punctuation.definition.entity.html",
				"string.quoted.double.php punctuation.definition.variable.php",
				"string.quoted.double.sql.php punctuation.definition.variable.php",
				"punctuation.definition.template-expression.begin.js",
				"punctuation.definition.template-expression.end.js"
			],
			"settings": {
				"foreground": "#e27013",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Escape Orange",
			"scope": [],
			"settings": {
				"foreground": "#e27013"
			}
		},
		{
			"name": "Pale Blue",
			"scope": [
				"support.function.magic.php",
				"support.function.constructor.php",
				"meta.object-literal.key",
				"meta.object-literal.key entity.name.function",
				"support.type.property-name",
				"support.type.property-name punctuation",
				"variable.other.property",
				"variable.other.object.property.js",
				"entity.other.attribute-name.attribute.css",
				"entity.other.attribute-name.attribute.scss",
				"meta.property-name.media-query.css",
				"meta.property-name.media-query.scss"
			],
			"settings": {
				"foreground": "#9acfd1",
				"fontStyle": ""
			}
		},
		{
			"name": "Paler Blue",
			"scope": [
				"entity.name.tag.html",
				"entity.name.tag.xml",
				"entity.name.tag.localname.xml",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"punctuation.definition.tag.xml",
				"entity.other.attribute-name.html",
				"entity.other.attribute-name.xml",
				"entity.other.attribute-name.localname.xml",
				"punctuation.separator.key-value.html",
				"punctuation.definition.string.begin.html",
				"punctuation.definition.string.end.html",
				"punctuation.definition.string.begin.xml",
				"punctuation.definition.string.end.xml"
			],
			"settings": {
				"foreground": "#a8c4c5",
				"fontStyle": ""
			}
		},
		{
			"name": "White",
			"scope": [
				"entity.name.function",
				"support.function.misc.scss",
				"constant.numeric.css"
			],
			"settings": {
				"foreground": "#fff",
				"fontStyle": ""
			}
		},
		{
			"name": "White Bold",
			"scope": ["keyword.operator", "entity.name.tag.reference.scss"],
			"settings": {
				"foreground": "#fff",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Purple",
			"scope": [
				"entity.other.attribute-name.pseudo-class.css",
				"entity.other.attribute-name.pseudo-element.css",
				"meta.tag.metadata.doctype entity.name.tag.html"
			],
			"settings": {
				"foreground": "#b587cf"
			}
		},
		{
			"name": "Bold Purple",
			"scope": [
				"storage.modifier",
				"keyword.control.at-rule.media.css",
				"keyword.control.at-rule.media.scss",
				"keyword.control.at-rule.supports.css",
				"keyword.control.at-rule.supports.scss"
			],
			"settings": {
				"foreground": "#a478bd",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Pale Red",
			"scope": [],
			"settings": {
				"foreground": "#dd3e3e"
			}
		},
		{
			"name": "Error Red",
			"scope": ["invalid", "invalid.illegal"],
			"settings": {
				"foreground": "#b33e3e",
				"fontStyle": "italic bold"
			}
		},
		{
			"name": "Pale Pink",
			"scope": ["entity.other.attribute-name.placeholder.css"],
			"settings": {
				"foreground": "#c27e9e"
			}
		},
		{
			"name": "Pale Red Bold",
			"scope": [
				"entity.name.type.trait.php",
				"entity.name.type.class.php",
				"entity.name.type.class.js",
				"entity.other.inherited-class.php",
				"entity.name.type.interface.php",
				"support.class",
				"constant.language",
				"variable.other.global.php",
				"variable.other.global.php punctuation.definition.variable.php",
				"variable.other.global.safer.php",
				"variable.other.global.safer.php punctuation.definition.variable.php",
				"variable.language.this.php",
				"variable.language.this.php punctuation.definition.variable.php",
				"support.constant.ext.php",
				"support.constant.core.php",
				"support.constant.std.php",
				"entity.other.alias.php",
				"support.constant",
				"constant.other.php",
				"constant.other.class.php",
				"variable.language.this.js",
				"entity.name.type.js",
				"variable.other.constant.property.js",
				"variable.other.constant.object.js"
			],
			"settings": {
				"foreground": "#dd3e3e",
				"fontStyle": "bold"
			}
		}
	]
}
